{
  "presets": ["next/babel"],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    [
      "module-resolver",
      {
        "alias": {
          "Test": "./test",
          "Components": "./components",
          "Helpers": "./helpers",
          "Static": "./static",
          "Layout": "./Layout"
        }
      }
    ]
  ]
}
